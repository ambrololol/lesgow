<!-- date time picker utk deadline -->
<script>
  $(function () {
    $('input[name="deadlineTugas"]').daterangepicker({
      singleDatePicker: true,
      timePicker: true,
      timePicker24Hour: true,
      endDate: moment().startOf("hour").add(32, "hour"),
      locale: {
        format: "YYYY-MM-DD H:mm:ss",
      },
    });
  });
</script>

