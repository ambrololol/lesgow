<script>
  var socket = io({
    autoConnect: true
  })

  socket.on("connect",()=>{
    console.log("OK")
    let user = <%- JSON.stringify(user) %>
    console.log("User", user)
    let data = {
      email: user.email
    }
    socket.emit("device-connected", data)
  })
</script>
